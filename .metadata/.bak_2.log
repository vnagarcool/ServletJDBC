		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		try {
			//1. Load the driver
			Class.forName("com.mysql.jdbc.Driver");
			//Class.forName("com.mysql.cj.jdbc.Driver");
		
			//2. connecting to the database
			String url = "jdbc:mysql://localhost:3306/trail";
